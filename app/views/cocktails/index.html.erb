<div class="container">
  <div class="row">
    <div class="col-xs-6 col-xs-offset-3 text-center">
        <h2>Here is your cocktail list</h2>
        <ul class="list-unstyled">
          <% @cocktails.each do |cocktail| %>
            <li><%= link_to cocktail.name, cocktail_path(cocktail) %></li>
          <% end %>
        </ul>
    </div>
  </div>
</div>

